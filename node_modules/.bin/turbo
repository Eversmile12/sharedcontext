#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vitto/Projects/sharme/node_modules/.pnpm/@ardrive+turbo-sdk@1.40.2_@solana+sysvars@5.5.1_typescript@5.9.3__@tanstack+query-core@_e5cb67bad18592ab28ffb495553818cd/node_modules/@ardrive/turbo-sdk/lib/esm/cli/node_modules:/Users/vitto/Projects/sharme/node_modules/.pnpm/@ardrive+turbo-sdk@1.40.2_@solana+sysvars@5.5.1_typescript@5.9.3__@tanstack+query-core@_e5cb67bad18592ab28ffb495553818cd/node_modules/@ardrive/turbo-sdk/lib/esm/node_modules:/Users/vitto/Projects/sharme/node_modules/.pnpm/@ardrive+turbo-sdk@1.40.2_@solana+sysvars@5.5.1_typescript@5.9.3__@tanstack+query-core@_e5cb67bad18592ab28ffb495553818cd/node_modules/@ardrive/turbo-sdk/lib/node_modules:/Users/vitto/Projects/sharme/node_modules/.pnpm/@ardrive+turbo-sdk@1.40.2_@solana+sysvars@5.5.1_typescript@5.9.3__@tanstack+query-core@_e5cb67bad18592ab28ffb495553818cd/node_modules/@ardrive/turbo-sdk/node_modules:/Users/vitto/Projects/sharme/node_modules/.pnpm/@ardrive+turbo-sdk@1.40.2_@solana+sysvars@5.5.1_typescript@5.9.3__@tanstack+query-core@_e5cb67bad18592ab28ffb495553818cd/node_modules/@ardrive/node_modules:/Users/vitto/Projects/sharme/node_modules/.pnpm/@ardrive+turbo-sdk@1.40.2_@solana+sysvars@5.5.1_typescript@5.9.3__@tanstack+query-core@_e5cb67bad18592ab28ffb495553818cd/node_modules:/Users/vitto/Projects/sharme/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vitto/Projects/sharme/node_modules/.pnpm/@ardrive+turbo-sdk@1.40.2_@solana+sysvars@5.5.1_typescript@5.9.3__@tanstack+query-core@_e5cb67bad18592ab28ffb495553818cd/node_modules/@ardrive/turbo-sdk/lib/esm/cli/node_modules:/Users/vitto/Projects/sharme/node_modules/.pnpm/@ardrive+turbo-sdk@1.40.2_@solana+sysvars@5.5.1_typescript@5.9.3__@tanstack+query-core@_e5cb67bad18592ab28ffb495553818cd/node_modules/@ardrive/turbo-sdk/lib/esm/node_modules:/Users/vitto/Projects/sharme/node_modules/.pnpm/@ardrive+turbo-sdk@1.40.2_@solana+sysvars@5.5.1_typescript@5.9.3__@tanstack+query-core@_e5cb67bad18592ab28ffb495553818cd/node_modules/@ardrive/turbo-sdk/lib/node_modules:/Users/vitto/Projects/sharme/node_modules/.pnpm/@ardrive+turbo-sdk@1.40.2_@solana+sysvars@5.5.1_typescript@5.9.3__@tanstack+query-core@_e5cb67bad18592ab28ffb495553818cd/node_modules/@ardrive/turbo-sdk/node_modules:/Users/vitto/Projects/sharme/node_modules/.pnpm/@ardrive+turbo-sdk@1.40.2_@solana+sysvars@5.5.1_typescript@5.9.3__@tanstack+query-core@_e5cb67bad18592ab28ffb495553818cd/node_modules/@ardrive/node_modules:/Users/vitto/Projects/sharme/node_modules/.pnpm/@ardrive+turbo-sdk@1.40.2_@solana+sysvars@5.5.1_typescript@5.9.3__@tanstack+query-core@_e5cb67bad18592ab28ffb495553818cd/node_modules:/Users/vitto/Projects/sharme/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@ardrive+turbo-sdk@1.40.2_@solana+sysvars@5.5.1_typescript@5.9.3__@tanstack+query-core@_e5cb67bad18592ab28ffb495553818cd/node_modules/@ardrive/turbo-sdk/lib/esm/cli/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/@ardrive+turbo-sdk@1.40.2_@solana+sysvars@5.5.1_typescript@5.9.3__@tanstack+query-core@_e5cb67bad18592ab28ffb495553818cd/node_modules/@ardrive/turbo-sdk/lib/esm/cli/cli.js" "$@"
